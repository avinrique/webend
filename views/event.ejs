  <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

<div class="w3-container w3-teal">
  <h1><%=data.name%></h1>
</div>

<img src="" alt="img" style="width:100%" id="seeingimage">
<script>
  const seingimg = document.getElementById("seeingimage").src
  if(seingimg == "http://localhost:3000/events/6289ed9cd446661955068d01"){
    document.getElementById("seeingimage").setAttribute("hidden" , true)
  }

</script>

<div class="w3-container">

  <p>description : <%=data.description%></p>
  <p>duration : <%=data.duration%> days</p>
  <p>difficulty : <%=data.difficulty%></p>
  <p>grpsize : <%=data.grpsize%></p>
  <p>location : <%=data.startLocation%></p>
  <p><%=data.create%></p>

</div>

<div class="w3-container w3-teal">
  <p>The Reviews : </p>
  <div>
  <% reviewscount = data.reviews                    %> 
  <%reviewscount.forEach(datas => {%> 
    <div style="color: white; background-color: black;display: inline-block;">
    <p>review : <%=datas.review%></p><br>
    <p>rating : <%=datas.rating%></p><br>
    <p>user: <%=datas.user.username%></p><br>
    
  </div>
  <% }) %>
</div>
